<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.mapper.UserJobMapper">
    <select id="getUserJobInfo" parameterType="java.lang.String" resultType="com.example.demo.eneity.UserBaseInfo">
        select * from user_job_info where user_id = #{userId} and is_deleted = 0
